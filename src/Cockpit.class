package harvester;

protected class Cockpit implements ICockpit{

    private boolean keyTurned;

    protected void turnKey(){
        keyTurned = true;
    };

    protected void takeKeyOut(){              
        keyTurned = false;
    };

    protected void turnLeft(){
        System.out.write("turned left");
    };

    protected void turnRight(){
        System.out.write("turned right");
    };

    protected void forward(){
        System.out.write("drive forward");
    };

    protected void backward(){
        System.out.write("drive backward");
    };

    protected void stop(){
        System.out.write("stop");
    };
    
}